<form>
    <label name="request"/>
    <field name="request" widget="char"/>
    <label name="date"/>
    <group id="date_and_ugent" colspan="1">
      <field name="date"/>
      <label name="urgent"/>
      <field name="urgent"/>
    </group>
    <label name="source_type"/>
    <group id="lab_sample_source" colspan="1" col="0">
      <field name="source_type"/>
      <field name="patient_id"/>
      <field name="gender_str"/>
      <label name="age_num"/>
      <field name="age_num"/>
      <field name="other_source"/>
    </group>
    <label name="test_type"/>
    <field name="test_type"/>
    <label name="specimen_type"/>
    <field name="specimen_type"/>
    <label name="doctor_id"/>
    <field name="doctor_id"/>
    <label name="context"/>
    <field name="context"/>
    <label name="state"/>
    <field name="state" readonly="1"/>
    <label name="comment" xalign="0.0" colspan="4"/>
    <field name="comment" colspan="4"/>
</form>
