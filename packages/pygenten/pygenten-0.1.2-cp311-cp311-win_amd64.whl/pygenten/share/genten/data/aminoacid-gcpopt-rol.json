{
    "solver-method": "gcp-opt",
    "timings": true,
    "tensor":
    {
        "input-file": "data/aminoacid_data_dense.txt",
        "format": "dense"
    },
    "k-tensor":
    {
        "rank": 15,
        "output-file": "aminoacid.ktn",
        "initial-guess": "rand",
        "distributed-guess": "serial",
        "seed": 12345,
        "prng": false
    },
    "gcp-opt":
    {
        "method": "rol",
        "maxiters": 200,
        "gtol": 1e-4,
        "type": "gaussian",
        "fit": true
    },
    "testing":
    {
        "final-fit":
        {
            "value": 0.99,
            "absolute-tolerance": 0.05
        },
        "iterations":
        {
            "value": 68,
            "absolute-tolerance": 2
        }
    }
}
