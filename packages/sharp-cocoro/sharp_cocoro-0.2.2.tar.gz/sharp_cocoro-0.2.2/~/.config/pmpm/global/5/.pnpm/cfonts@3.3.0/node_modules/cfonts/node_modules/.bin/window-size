#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/david/src/sharp-cocoro/~/.config/pmpm/global/5/.pnpm/window-size@1.1.1/node_modules/window-size/node_modules:/Users/david/src/sharp-cocoro/~/.config/pmpm/global/5/.pnpm/window-size@1.1.1/node_modules:/Users/david/src/sharp-cocoro/~/.config/pmpm/global/5/.pnpm/node_modules:/Users/david/src/sharp-cocoro/~/.config/pmpm/global/5/node_modules:/Users/david/src/sharp-cocoro/~/.config/pmpm/global/node_modules:/Users/david/src/sharp-cocoro/~/.config/pmpm/node_modules:/Users/david/src/sharp-cocoro/~/.config/node_modules:/Users/david/src/sharp-cocoro/~/node_modules:/Users/david/src/sharp-cocoro/node_modules:/Users/david/src/node_modules:/Users/david/node_modules:/Users/node_modules:/node_modules:~/.config/pmpm/global/5/.pnpm/node_modules"
else
  export NODE_PATH="/Users/david/src/sharp-cocoro/~/.config/pmpm/global/5/.pnpm/window-size@1.1.1/node_modules/window-size/node_modules:/Users/david/src/sharp-cocoro/~/.config/pmpm/global/5/.pnpm/window-size@1.1.1/node_modules:/Users/david/src/sharp-cocoro/~/.config/pmpm/global/5/.pnpm/node_modules:/Users/david/src/sharp-cocoro/~/.config/pmpm/global/5/node_modules:/Users/david/src/sharp-cocoro/~/.config/pmpm/global/node_modules:/Users/david/src/sharp-cocoro/~/.config/pmpm/node_modules:/Users/david/src/sharp-cocoro/~/.config/node_modules:/Users/david/src/sharp-cocoro/~/node_modules:/Users/david/src/sharp-cocoro/node_modules:/Users/david/src/node_modules:/Users/david/node_modules:/Users/node_modules:/node_modules:~/.config/pmpm/global/5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../window-size@1.1.1/node_modules/window-size/cli.js" "$@"
else
  exec node  "$basedir/../../../../../window-size@1.1.1/node_modules/window-size/cli.js" "$@"
fi
