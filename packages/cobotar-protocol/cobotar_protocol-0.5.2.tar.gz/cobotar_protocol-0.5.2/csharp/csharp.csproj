<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>CobotAR Protocol</Title>
    <Version>0.5.2</Version>
    <Authors>Rasmus Lunding</Authors>
    <Company>cobotar.org</Company>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>cobotar.protocol</PackageId>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/cobotar/protocol</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.31.1" />
  </ItemGroup>

</Project>
