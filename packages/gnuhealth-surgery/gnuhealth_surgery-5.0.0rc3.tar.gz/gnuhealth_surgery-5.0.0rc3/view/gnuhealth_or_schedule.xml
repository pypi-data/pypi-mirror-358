<form>
    <group id="or_shcedule_header" string="Main" colspan="4" col="4">
        <label name="oproom"/>
        <field name="oproom"/>
        <label name="surgery"/>
        <field name="surgery"/>
        <label name="reserve_from"/>
        <field name="reserve_from"/>
        <label name="reserve_to"/>
        <field name="reserve_to"/>
    </group>
    <group id="or_schedule_details" string="Details" colspan="4" col="6">
        <label name="patient"/>
        <field name="patient"/>
        <label name="health_condition"/>
        <field name="health_condition" colspan="3"/>
        <label name="healthprof"/>
        <field name="healthprof"/>
        <label name="specialty"/>
        <field name="specialty"/>
        <label name="urgency"/>
        <field name="urgency"/>
    </group>
    <newline/>
    <separator id="or_schedule_remarks" string="Remarks" colspan="4"/>
    <newline/>
    <field name="comments" colspan="4"/>
    <group id="or_shcedule_footer" colspan="4">
        <label name="institution"/>
        <field name="institution"/>
        <label name="state"/>
        <field name="state"/>
    </group>
</form>
