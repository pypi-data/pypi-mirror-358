# Simple test for Chichewa operators
ika x = 5
ika y = 10

# Test basic comparison
ngati x wafanana 5 chita
    onetsa("x equals 5")

ngati x wasiyana y chita
    onetsa("x not equal to y")
    
ngati y wapambana x chita
    onetsa("y greater than x")
    
ngati x wachepa y chita
    onetsa("x less than y")

