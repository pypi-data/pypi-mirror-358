<script>
  try {
    if(document.body.className.includes("hex-dark")) {
      const style = document.createElement('style')
      style.innerText = `
        body {
          background-color: rgb(30, 30, 40);
          color: rgb(228, 230, 236);
        }
        a, a:visited {
          color: rgb(100, 181, 246);
        }
      `
      document.head.appendChild(style)
    }
  } catch (err) {
    console.log("Modelbit: Unable to copy Hex body color", err)
  }
</script>
