# netcup CLI Configuration
# Copy this file to .env and fill in your actual credentials

# Required: Your netcup credentials
NETCUP_CUSTOMER_NUMBER=your_customer_number_here
NETCUP_API_KEY=your_api_key_here
NETCUP_API_PASSWORD=your_api_password_here

# Optional: API endpoint (use default if not specified)
# NETCUP_API_ENDPOINT=https://ccp.netcup.net/run/webservice/servers/endpoint.php?JSON 