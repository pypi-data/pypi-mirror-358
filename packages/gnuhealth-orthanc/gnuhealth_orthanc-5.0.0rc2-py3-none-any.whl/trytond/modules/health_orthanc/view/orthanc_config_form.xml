<form>
    <group string="Server Information" id="orthanc_group_server_info">
        <group string="General" yfill="1" id="orthanc_group_general_cofig">
            <label name="label"/>
            <field name="label"/>
            <label name="domain"/>
            <field name="domain"/>
            <label name="proxy_domain"/>
            <field name="proxy_domain"/>
            <label name="validated"/>
            <field name="validated" readonly="1"/>
            <label name="link" string="Link"/>
            <field name="link" widget="url"/>
        </group>
        <newline/>
        <separator string=" " id="blank_line"/>
        <newline/>
        <group string="User" yfill="1" id="orthanc_group_user_config">
            <label name="user"/>
            <field name="user"/>
            <label name="password"/>
            <field name="password"/>
        </group>
    </group>
</form>