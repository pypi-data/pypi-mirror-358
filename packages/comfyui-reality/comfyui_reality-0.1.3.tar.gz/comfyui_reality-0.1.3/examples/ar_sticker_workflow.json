{
  "name": "AR Sticker - Cute Robot",
  "description": "Creates an AR sticker of a cute robot character for Reality Composer",
  
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "sd_xl_base_1.0.safetensors"
    }
  },
  
  "2": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "cute chibi robot character, kawaii style, flat design, sticker art, vibrant colors, simple shapes, friendly expression, big eyes, rounded edges, pastel purple and mint green, white background",
      "clip": ["1", 1]
    }
  },
  
  "3": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "realistic, photographic, 3d render, complex details, shadows, dark, scary, sharp edges, text, watermark",
      "clip": ["1", 1]
    }
  },
  
  "4": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    }
  },
  
  "5": {
    "class_type": "KSampler",
    "inputs": {
      "seed": 2024,
      "steps": 25,
      "cfg": 7.5,
      "sampler_name": "dpmpp_2m",
      "scheduler": "karras",
      "denoise": 1.0,
      "model": ["1", 0],
      "positive": ["2", 0],
      "negative": ["3", 0],
      "latent_image": ["4", 0]
    }
  },
  
  "6": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["5", 0],
      "vae": ["1", 2]
    }
  },
  
  "7": {
    "class_type": "ImageSave",
    "inputs": {
      "images": ["6", 0],
      "filename_prefix": "ar_robot_original"
    }
  },
  
  "8": {
    "class_type": "AROptimizer",
    "inputs": {
      "image": ["6", 0],
      "optimization_level": "balanced",
      "target_platform": "ios",
      "texture_size": 1024,
      "simplification_ratio": 0.8,
      "alpha_mode": "cutout"
    }
  },
  
  "9": {
    "class_type": "MaterialComposer",
    "inputs": {
      "geometry": ["8", 0],
      "material_type": "unlit",
      "base_color": [1.0, 1.0, 1.0, 1.0],
      "double_sided": true,
      "alpha_cutoff": 0.5,
      "emissive_intensity": 0.0
    }
  },
  
  "10": {
    "class_type": "SpatialPositioner",
    "inputs": {
      "ar_object": ["9", 0],
      "position_x": 0.0,
      "position_y": 0.0,
      "position_z": 0.0,
      "rotation_x": 0.0,
      "rotation_y": 0.0,
      "rotation_z": 0.0,
      "scale": 0.15,
      "anchor_type": "horizontal",
      "coordinate_system": "y-up"
    }
  },
  
  "11": {
    "class_type": "USDZExporter",
    "inputs": {
      "ar_scene": ["10", 0],
      "filename": "cute_robot_sticker",
      "metadata": {
        "title": "Cute Robot AR Sticker",
        "author": "ComfyReality",
        "description": "A friendly robot character for AR",
        "keywords": ["robot", "sticker", "AR", "cute"]
      },
      "optimize_for_ar": true,
      "include_preview": true,
      "compression_level": "medium"
    }
  },
  
  "12": {
    "class_type": "PreviewImage",
    "inputs": {
      "images": ["6", 0]
    }
  }
}