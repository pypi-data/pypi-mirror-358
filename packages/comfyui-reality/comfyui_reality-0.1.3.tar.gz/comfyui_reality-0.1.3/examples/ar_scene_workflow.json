{
  "name": "AR Scene - Interactive Mascot",
  "description": "Creates an animated AR mascot with physics for Reality Composer",
  
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "sd_xl_base_1.0.safetensors"
    }
  },
  
  "2": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "adorable floating cloud character, kawaii mascot, fluffy white cloud with happy face, simple cartoon style, soft edges, sky blue accents, minimalist design, sticker style, centered composition",
      "clip": ["1", 1]
    }
  },
  
  "3": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "realistic, photograph, complex, detailed, scary, dark, text, logo, watermark",
      "clip": ["1", 1]
    }
  },
  
  "4": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    }
  },
  
  "5": {
    "class_type": "KSampler",
    "inputs": {
      "seed": 42424,
      "steps": 30,
      "cfg": 8.0,
      "sampler_name": "dpmpp_2m",
      "scheduler": "karras",
      "denoise": 1.0,
      "model": ["1", 0],
      "positive": ["2", 0],
      "negative": ["3", 0],
      "latent_image": ["4", 0]
    }
  },
  
  "6": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["5", 0],
      "vae": ["1", 2]
    }
  },
  
  "7": {
    "class_type": "AROptimizer",
    "inputs": {
      "image": ["6", 0],
      "optimization_level": "quality",
      "target_platform": "universal",
      "texture_size": 2048,
      "simplification_ratio": 0.9,
      "alpha_mode": "blend",
      "generate_lod": true,
      "lod_levels": 3
    }
  },
  
  "8": {
    "class_type": "MaterialComposer",
    "inputs": {
      "geometry": ["7", 0],
      "material_type": "standard",
      "base_color": [1.0, 1.0, 1.0, 1.0],
      "metallic": 0.0,
      "roughness": 0.8,
      "double_sided": true,
      "emissive_color": [0.5, 0.7, 1.0],
      "emissive_intensity": 0.3,
      "occlusion_strength": 0.5,
      "normal_scale": 0.5
    }
  },
  
  "9": {
    "class_type": "AnimationBuilder",
    "inputs": {
      "ar_object": ["8", 0],
      "animation_type": "float",
      "float_amplitude": 0.05,
      "float_frequency": 0.5,
      "rotation_enabled": true,
      "rotation_speed": 10.0,
      "rotation_axis": "y",
      "loop": true,
      "duration": 0.0
    }
  },
  
  "10": {
    "class_type": "PhysicsIntegrator",
    "inputs": {
      "ar_object": ["9", 0],
      "collision_shape": "sphere",
      "physics_type": "dynamic",
      "mass": 0.5,
      "restitution": 0.7,
      "friction": 0.3,
      "enable_gravity": false,
      "collision_group": "interactive"
    }
  },
  
  "11": {
    "class_type": "SpatialPositioner",
    "inputs": {
      "ar_object": ["10", 0],
      "position_x": 0.0,
      "position_y": 0.5,
      "position_z": 0.0,
      "rotation_x": 0.0,
      "rotation_y": 0.0,
      "rotation_z": 0.0,
      "scale": 0.2,
      "anchor_type": "horizontal",
      "coordinate_system": "y-up"
    }
  },
  
  "12": {
    "class_type": "RealityComposer",
    "inputs": {
      "ar_objects": [["11", 0]],
      "scene_name": "Cloud Mascot Scene",
      "ambient_intensity": 1.0,
      "enable_shadows": true,
      "shadow_intensity": 0.3,
      "enable_occlusion": true,
      "enable_people_occlusion": true,
      "ar_coaching": true,
      "allow_multiple": false
    }
  },
  
  "13": {
    "class_type": "CrossPlatformExporter",
    "inputs": {
      "ar_scene": ["12", 0],
      "export_format": "usdz",
      "filename": "cloud_mascot_interactive",
      "ios_config": {
        "ar_quick_look": true,
        "reality_composer": true,
        "min_ios_version": "13.0"
      },
      "compression": "balanced",
      "include_animations": true,
      "include_physics": true
    }
  }
}