{
  "name": "@cdmx/postgres-mcp-server",
  "version": "1.0.0",
  "description": "A Model Context Protocol server for PostgreSQL databases with SSL support",
  "main": "dist/index.js",
  "bin": {
    "postgres-mcp-server": "./dist/index.js",
    "postgres-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "postgresql",
    "database",
    "ai",
    "llm",
    "model-context-protocol"
  ],
  "author": "MCP Community <community@mcp.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcp-community/postgres-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/mcp-community/postgres-mcp-server/issues"
  },
  "homepage": "https://modelcontextprotocol.io",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
